{% extends "ExampleUserRegistrationBundle:UserRegistration:layout.html.twig" %}

{% block title %}{{ parent() }}ユーザー情報のご入力{% endblock %}

{% block content %}
<h2>ユーザー情報のご入力</h2>

<div class="well">
  <form action="{{ path('example_userregistration_userregistration_inputpost') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}

    <div class="form-actions">
      <button class="btn btn-primary span1" type="submit" name="next"><b>次へ</b></button>
    </div>
  </form>
</div>
{% endblock %}
